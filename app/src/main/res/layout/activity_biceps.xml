<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".BicepsActivity">

    <androidx.gridlayout.widget.GridLayout
        android:layout_width="411dp"
        android:layout_height="598dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp">

        <ImageView
            android:id="@+id/dumbbellBicepsCurl"
            android:layout_width="107dp"
            android:layout_height="81dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            app:layout_column="0"
            app:layout_row="0" />

        <ImageView
            android:id="@+id/dumbbellHammerCurl"
            android:layout_width="107dp"
            android:layout_height="81dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            app:layout_column="0"
            app:layout_row="1" />

        <ImageView
            android:id="@+id/dumbbellConcentrationCurl"
            android:layout_width="107dp"
            android:layout_height="81dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            app:layout_column="0"
            app:layout_row="2" />

        <ImageView
            android:id="@+id/ezBarbellCurl"
            android:layout_width="107dp"
            android:layout_height="81dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            app:layout_column="0"
            app:layout_row="3" />

        <ImageView
            android:id="@+id/cableCurl"
            android:layout_width="107dp"
            android:layout_height="81dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            app:layout_column="0"
            app:layout_row="4" />

        <ImageView
            android:id="@+id/dumbbellCloseGripCurl"
            android:layout_width="107dp"
            android:layout_height="81dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            app:layout_column="0"
            app:layout_row="5" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="250dp"
            android:layout_height="75dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/rounded_corner"
            android:text="Dumbbell Biceps Curl 3x12
                                            The dumbbell biceps curl primarily targets the biceps brachii, which is the muscle located on the front of the upper arm."
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="0" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="250dp"
            android:layout_height="75dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="14dp"
            android:textSize="12sp"
            android:background="@drawable/rounded_corner"
            android:text="Dumbbell Hammer Curl 3x12
                                            The dumbbell hammer curl primarily targets the brachialis muscle, which is located underneath the biceps brachii on the front of the upper arm."
            android:textColor="@color/black"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="1" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="250dp"
            android:layout_height="75dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="14dp"
            android:textSize="12sp"
            android:background="@drawable/rounded_corner"
            android:text="Dumbbel Concentration Curl 3x12
                                            The dumbbell concentration curl primarily targets the biceps brachii muscle."
            android:textColor="@color/black"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="2" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="250dp"
            android:layout_height="75dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="14dp"
            android:textSize="12sp"
            android:background="@drawable/rounded_corner"
            android:text="EZ Barbell Curl 3x12
                                                        The EZ barbell curl primarily targets the biceps brachii muscle, which is located on the front of the upper arm."
            android:textColor="@color/black"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="3" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="250dp"
            android:layout_height="75dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="14dp"
            android:textSize="12sp"
            android:background="@drawable/rounded_corner"
            android:text="Cable Curl 3x12
                                                                The cable curl primarily targets the biceps brachii muscle, located on the front of the upper arm."
            android:textColor="@color/black"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="4" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="250dp"
            android:layout_height="75dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="14dp"
            android:textSize="12sp"
            android:background="@drawable/rounded_corner"
            android:text="Dumbbell Close Grip Curl 3x12
                                            The dumbbell close grip curl primarily targets the biceps brachii muscle, which is located on the front of the upper arm."
            android:textColor="@color/black"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="5" />
    </androidx.gridlayout.widget.GridLayout>
</androidx.constraintlayout.widget.ConstraintLayout>