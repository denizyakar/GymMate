<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".AbsActivity">

    <androidx.gridlayout.widget.GridLayout
        android:layout_width="411dp"
        android:layout_height="598dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/sitUp"
            android:layout_width="107dp"
            android:layout_height="81dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            app:layout_column="0"
            app:layout_row="0" />

        <ImageView
            android:id="@+id/airBike"
            android:layout_width="107dp"
            android:layout_height="81dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            app:layout_column="0"
            app:layout_row="1" />

        <ImageView
            android:id="@+id/cableKneelingCrunch"
            android:layout_width="107dp"
            android:layout_height="81dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            app:layout_column="0"
            app:layout_row="2" />

        <ImageView
            android:id="@+id/armSlingersHangingBentKneeLegs"
            android:layout_width="107dp"
            android:layout_height="81dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            app:layout_column="0"
            app:layout_row="3" />

        <ImageView
            android:id="@+id/cableKneelingSideCrunch"
            android:layout_width="107dp"
            android:layout_height="81dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            app:layout_column="0"
            app:layout_row="4" />

        <ImageView
            android:id="@+id/verticalLegRaise"
            android:layout_width="107dp"
            android:layout_height="81dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            app:layout_column="0"
            app:layout_row="5" />

        <TextView
            android:id="@+id/textView28"
            android:layout_width="250dp"
            android:layout_height="75dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/rounded_corner"
            android:text="Sit Up 3x20
                                                                        The sit-up primarily targets the rectus abdominis muscle, which is the long muscle that runs vertically along the front of your abdomen."
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="0" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="250dp"
            android:layout_height="75dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/rounded_corner"
            android:text="Air Bike 3x15
                                                                            The air bike, also known as bicycle crunches or bicycle kicks, primarily targets the rectus abdominis muscle."
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="1" />

        <TextView
            android:id="@+id/textView30"
            android:layout_width="250dp"
            android:layout_height="75dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/rounded_corner"
            android:text="Cable Kneeling Crunch 3x20
                                                    The cable kneeling crunch primarily targets the rectus abdominis muscle, which is the long muscle that runs vertically along the front of your abdomen."
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="2" />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="250dp"
            android:layout_height="75dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/rounded_corner"
            android:text="Arm Slingers Hanging Bent Knee Legs 3x20
                                                    Arm slingers hanging bent knee legs primarily targets the lower abdominal muscles, including the lower portion of the rectus abdominis."
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="3" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="250dp"
            android:layout_height="75dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/rounded_corner"
            android:text="Cable Kneeling Side Crunch 3x20
                                    The cable kneeling side crunch primarily targets the obliques, which are the muscles on the sides of the abdomen."
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="4" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="250dp"
            android:layout_height="75dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/rounded_corner"
            android:text="Vertical Leg Raise 3x20
                                                    The vertical leg raise primarily targets the lower abdominals, specifically the lower portion of the rectus abdominis muscle."
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_column="1"
            app:layout_row="5" />
    </androidx.gridlayout.widget.GridLayout>
</androidx.constraintlayout.widget.ConstraintLayout>